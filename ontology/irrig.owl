    <!-- http://www.w3id.org/def/irrig#RainIntensittyState -->

    <owl:Class rdf:about="http://www.w3id.org/def/irrig#RainIntensittyState">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.w3id.org/def/caso#State"/>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.w3id.org/def/caso#hasClosedLowerBoundary"/>
                                <owl:someValuesFrom rdf:resource="http://www.w3id.org/def/irrig#RainIntensityBoundary"/>
                            </owl:Restriction>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.w3id.org/def/caso#hasOpenLowerBoundary"/>
                                <owl:someValuesFrom rdf:resource="http://www.w3id.org/def/irrig#RainIntensityBoundary"/>
                            </owl:Restriction>
                        </owl:unionOf>
                    </owl:Class>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.w3id.org/def/caso#hasClosedUpperBoundary"/>
                                <owl:someValuesFrom rdf:resource="http://www.w3id.org/def/irrig#RainIntensityBoundary"/>
                            </owl:Restriction>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.w3id.org/def/caso#hasOpenUpperBoundary"/>
                                <owl:someValuesFrom rdf:resource="http://www.w3id.org/def/irrig#RainIntensityBoundary"/>
                            </owl:Restriction>
                        </owl:unionOf>
                    </owl:Class>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.w3id.org/def/caso#State"/>
        <rdfs:comment xml:lang="en">Situation that a rain intensity level has qualitative data that distinguish with other situation. This is a definitive class including the following states: init, light, moderate, heavy.</rdfs:comment>
        <rdfs:label xml:lang="en">Rain Intensity State</rdfs:label>
    </owl:Class>
    


