<http://www.w3id.org/def/irrig> rdf:type owl:Ontology ;
                                 owl:versionIRI <http://www.w3id.org/def/irrig/201905> ;
                                 owl:imports <https://raw.githubusercontent.com/Irstea/caso/master/ontology/caso.owl> ;
                                 <http://purl.org/dc/elements/1.1/description> "IRRIG is an Irrigation Ontology for experimentation in TSCF, Irstea Clermont-Fd."@en ;
                                 <http://purl.org/dc/elements/1.1/publisher> <https://www.irstea.fr/en/research/research-units/tscf> ;
                                 <http://purl.org/dc/elements/1.1/title> "Irrigation ontology for experimentation in TSCF"@en ;
                                 <http://purl.org/dc/terms/contributor> <http://orcid.org/0000-0002-7011-4535> ,
                                                                        "https://orcid.org/0000-0002-0421-452X" ;
                                 <http://purl.org/dc/terms/creator> <https://orcid.org/0000-0002-3076-5499> ,
                                                                    <https://orcid.org/0000-0002-3517-0945> ,
                                                                    <https://orcid.org/0000-0003-3587-0367> ;
                                 <http://purl.org/dc/terms/license> <http://purl.org/NET/rdflicense/cc-by4.0> ;
                                 <http://purl.org/vocab/vann/preferredNamespacePrefix> "irrig" ;
                                 <http://purl.org/vocab/vann/preferredNamespaceUri> "https://w3id.org/def/irrig#" ;
                                 owl:versionInfo "1.0.0" .

#################################################################
#    Annotation properties
#################################################################

###  http://www.w3.org/2004/02/skos/core#definition
<http://www.w3.org/2004/02/skos/core#definition> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#example
<http://www.w3.org/2004/02/skos/core#example> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#note
<http://www.w3.org/2004/02/skos/core#note> rdf:type owl:AnnotationProperty .


#################################################################
#    Data properties
#################################################################

###  https://w3id.org/saref#hasManufacturer
<https://w3id.org/saref#hasManufacturer> rdf:type owl:DatatypeProperty ;
                                         rdfs:range xsd:string ;
                                         rdfs:comment "A relationship identifying the manufacturer of an entity (e.g., device)" ;
                                         rdfs:label "has manufacturer" .


###  https://w3id.org/saref#hasModel
<https://w3id.org/saref#hasModel> rdf:type owl:DatatypeProperty ;
                                  rdfs:range xsd:string ;
                                  rdfs:comment "A relationship identifying the model of an entity (e.g., device)" ;
                                  rdfs:label "has model" .


###  https://w3id.org/saref#hasName
<https://w3id.org/saref#hasName> rdf:type owl:DatatypeProperty ;
                                 rdfs:range xsd:string ;
                                 rdfs:comment "A relationship providing the name of an entity (e.g., device)" ;
                                 rdfs:label "has name" .


#################################################################
#    Classes
#################################################################

###  http://www.w3id.org/def/irrig#AmountProperty
:AmountProperty rdf:type owl:Class ;
                rdfs:subClassOf <http://www.w3.org/ns/ssn/Property> ;
                rdfs:comment "AmountProperty - Amount aspect of an entity representing the quantity of the entity." ;
                rdfs:label "Amount Property" .


###  http://www.w3id.org/def/irrig#CropGrowthDeduction
:CropGrowthDeduction rdf:type owl:Class ;
                     owl:equivalentClass [ owl:intersectionOf ( <http://www.w3id.org/def/caso#Deduction>
                                                                [ rdf:type owl:Restriction ;
                                                                  owl:onProperty <http://www.w3.org/ns/sosa/hasFeatureOfInterest> ;
                                                                  owl:hasValue :FeatureOfInterest_Crop
                                                                ]
                                                                [ rdf:type owl:Restriction ;
                                                                  owl:onProperty <http://www.w3.org/ns/sosa/observedProperty> ;
                                                                  owl:hasValue :ObservableProperty_Crop_Growth
                                                                ]
                                                              ) ;
                                           rdf:type owl:Class
                                         ] ;
                     rdfs:subClassOf <http://www.w3id.org/def/caso#Deduction> ;
                     rdfs:comment "CropGrowthDeduction - a defined class represents act of carrying out an (Observation) Procedure to estimate a State of the irrig:ObservableProperty_Crop_Growth of the irrig:FeatureOfInterest_Crop. The Observation may be performed by an inference engine. Links to a State to describe what the result is. Links to a TemporalEntity to indicate when the result is valid and for how many time."@en ;
                     rdfs:label "Crop Growth Deduction"@en ;
                     <http://www.w3.org/2004/02/skos/core#example> "A CropGrowthDeduction is made by the Software_Agent Sensor based on human observation"@en .


###  http://www.w3id.org/def/irrig#CropGrowthObservation
:CropGrowthObservation rdf:type owl:Class ;
                       owl:equivalentClass [ owl:intersectionOf ( <http://www.w3id.org/def/caso#Observation>
                                                                  [ rdf:type owl:Restriction ;
                                                                    owl:onProperty <http://www.w3.org/ns/sosa/hasFeatureOfInterest> ;
                                                                    owl:hasValue :FeatureOfInterest_Crop
                                                                  ]
                                                                  [ rdf:type owl:Restriction ;
                                                                    owl:onProperty <http://www.w3.org/ns/sosa/observedProperty> ;
                                                                    owl:hasValue :ObservableProperty_Crop_Growth
                                                                  ]
                                                                ) ;
                                             rdf:type owl:Class
                                           ] ;
                       rdfs:subClassOf <http://www.w3id.org/def/caso#Observation> ;
                       rdfs:comment "CropGrowthObservation - a defined class that represents act of carrying out an (Observation) Procedure to estimate or calculate a value of the irrig:ObservableProperty_Crop_Growth of the irrig:FeatureOfInterest_Crop. This act can be performed during the context acquisition or context processing phases of a context aware system. Links to a Sensor to describe what made the Observation and how; links to the irrig:ObservableProperty_Crop_Growth to describe what the result is an estimate of, and to the irrig:FeatureOfInterest_Crop. When the act is made during the context processing phase the entry values of the procedure should be linked to the Observation by the wasDerivedFrom property."@en ;
                       rdfs:label "Crop Growth Observation"@en ;
                       <http://www.w3.org/2004/02/skos/core#example> "A CropGrowthObservation that made by the CatherineRoussey Sensor (human observation or biology sensor)."@en .


###  http://www.w3id.org/def/irrig#CropGrowthState
:CropGrowthState rdf:type owl:Class ;
                 rdfs:subClassOf <http://www.w3id.org/def/caso#State> ;
                 rdfs:comment "CropGrowthState - A qualitative value of the CropGrowth Property which changes over time, summarizing a set of information about that Property. To evaluate the State of the Property, some Boundaries are defined."@en ;
                 rdfs:label "Crop Growth State"@en ;
                 <http://www.w3.org/2004/02/skos/core#example> "A State V2 is linked to an individual of  CropGrowth Property dedicated to maize crop. The V2 State is reached when two leaves are fully emerged with collars visible in a maize plant. The State appearance could be evaluated by human observation or by automatic method like the Growing Degree Unit (GDU)."@en .


###  http://www.w3id.org/def/irrig#CropWaterNeedDeduction
:CropWaterNeedDeduction rdf:type owl:Class ;
                        owl:equivalentClass [ owl:intersectionOf ( <http://www.w3id.org/def/caso#Deduction>
                                                                   [ rdf:type owl:Restriction ;
                                                                     owl:onProperty <http://www.w3.org/ns/sosa/hasFeatureOfInterest> ;
                                                                     owl:hasValue :FeatureOfInterest_Crop
                                                                   ]
                                                                   [ rdf:type owl:Restriction ;
                                                                     owl:onProperty <http://www.w3.org/ns/sosa/observedProperty> ;
                                                                     owl:hasValue :ObservableProperty_Crop_WaterNeed
                                                                   ]
                                                                 ) ;
                                              rdf:type owl:Class
                                            ] ;
                        rdfs:subClassOf <http://www.w3id.org/def/caso#Deduction> ;
                        rdfs:comment "CropWaterNeedDeduction - a defined class represents act of carrying out an (Observation) Procedure to estimate a State of the irrig:ObservableProperty_Crop_WaterNeed of the irrig:FeatureOfInterest_Crop. The Observation may be performed by an inference engine. Links to a State to describe what the result is. Links to a TemporalEntity to indicate when the result is valid and for how many time."@en ;
                        rdfs:label "Crop Water Need Deduction"@en ;
                        <http://www.w3.org/2004/02/skos/core#example> "A CropWaterNeedDeduction that made by the Software_Agent Sensor based on observations made by tensiometer sensor"@en .


###  http://www.w3id.org/def/irrig#CropWaterNeedState
:CropWaterNeedState rdf:type owl:Class ;
                    rdfs:subClassOf <http://www.w3id.org/def/caso#State> ;
                    rdfs:comment "CropWaterNeedState - A qualitative value of the CropWaterNeed Property which changes over time, summarizing a set of information about that Property. To evaluate the State of the Property, some Boundaries are defined."@en ;
                    rdfs:label "Crop Water Need State"@en ;
                    <http://www.w3.org/2004/02/skos/core#example> "Example: The State No of the CropWaterNeed Property represents the fact that the crop does not need water. The State Yes of the CropWaterNeed Property represents the fact that the crop needs extra water."@en .


###  http://www.w3id.org/def/irrig#GrowthProperty
:GrowthProperty rdf:type owl:Class ;
                rdfs:subClassOf <http://www.w3.org/ns/ssn/Property> ;
                rdfs:comment "GrowthProperty - Growth aspect (characteristic dimension) of an entity representing the physical development or the increasing in amount, number or size of the entity. The Growth aspect of an entity is intrinsic to and cannot exist without the entity. The Growth quality values should be expressed by a qualitative value such as States or by a quantitative value."@en ;
                rdfs:label "Growth Property"@en ;
                <http://www.w3.org/2004/02/skos/core#example> "CropGrowth is a GrowthProperty representing the physical development of crop."@en .


###  http://www.w3id.org/def/irrig#IntensityProperty
:IntensityProperty rdf:type owl:Class ;
                   rdfs:subClassOf <http://www.w3.org/ns/ssn/Property> ;
                   rdfs:comment "IntensityProperty - Intensity aspect of an entity representing the strength effect or the level of power per unit area of the entity. The Intensity aspect of the entity is intrinsic to and cannot exist without the entity. The Intensity quality value should be expressed by a qualitative value such as States or by a quantitative value."@en ;
                   rdfs:label "Intensity Property"@en ;
                   <http://www.w3.org/2004/02/skos/core#example> "RainIntensity is a IntensityProperty representing the strength effect of rain."@en .


###  http://www.w3id.org/def/irrig#IrrigationActuation
:IrrigationActuation rdf:type owl:Class ;
                     owl:equivalentClass [ rdf:type owl:Class ;
                                           owl:unionOf ( [ owl:intersectionOf ( <http://www.w3id.org/def/caso#Actuation>
                                                                                [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                                                         owl:onProperty <http://www.w3.org/ns/sosa/actsOnProperty> ;
                                                                                                         owl:hasValue :ObservableProperty_Soil60cmDepth_Moisture
                                                                                                       ]
                                                                                                       [ rdf:type owl:Restriction ;
                                                                                                         owl:onProperty <http://www.w3.org/ns/sosa/hasFeatureOfInterest> ;
                                                                                                         owl:hasValue :FeatureOfInterest_Soil60cmDepth
                                                                                                       ]
                                                                                                     ) ;
                                                                                  rdf:type owl:Class
                                                                                ]
                                                                              ) ;
                                                           rdf:type owl:Class
                                                         ]
                                                         [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                                  owl:onProperty <http://www.w3.org/ns/sosa/actsOnProperty> ;
                                                                                  owl:hasValue :ObservableProperty_Soil30cmDepth_Moisture
                                                                                ]
                                                                                [ rdf:type owl:Restriction ;
                                                                                  owl:onProperty <http://www.w3.org/ns/sosa/hasFeatureOfInterest> ;
                                                                                  owl:hasValue :FeatureOfInterest_Soil30cmDepth
                                                                                ]
                                                                              ) ;
                                                           rdf:type owl:Class
                                                         ]
                                                       )
                                         ] ;
                     rdfs:subClassOf <http://www.w3id.org/def/caso#Actuation> ;
                     rdfs:comment "IrrigationActuation - Act of carrying out an Irrigation (Actuation) Procedure to change the State of a Moisture ActuatableProperty of a Soil FeatureOfInterest using a watering Actuator. This act is performed during the context exploitation phase of a context aware system. Links to an Actuator to describe what made the Actuation and how; links to an ActuatableProperty to describe the target that the Actuation change; links to a FeatureOfInterest to detail what that property was associated with. When the act is performed based on a Deduction, it should be linked to this Deduction by the wasInfluencedBy property. Links to the Function that is triggered by the Actuation."@en ;
                     rdfs:label "Irrigation actuation"@en ;
                     <http://www.w3.org/2004/02/skos/core#example> "An IrrigationActuation is made by Watering Valve Actuator, links to Soil30cmDepth FeatureOfInterest and its Soil30cmDepthMoisture ActuatableProperty."@en .


###  http://www.w3id.org/def/irrig#MoistureProperty
:MoistureProperty rdf:type owl:Class ;
                  rdfs:subClassOf <http://www.w3.org/ns/ssn/Property> ;
                  rdfs:comment "MoistureProperty - Moisture aspect of an entity representing the amount of water available inside the entity. The Moisture aspect of the entity is intrinsic to and cannot exist without the entity. The Moisture quality value should be expressed by a qualitative value such as States or by a quantitative value."@en ;
                  rdfs:label "Moisture Property"@en ;
                  <http://www.w3.org/2004/02/skos/core#example> "RootZoneMoistureLevel is a MoistureProperty representing the amount of water available around the soil’s root zone area of a plant."@en .


###  http://www.w3id.org/def/irrig#RainIntensityBoundary
:RainIntensityBoundary rdf:type owl:Class ;
                       rdfs:subClassOf <http://www.w3id.org/def/caso#Boundary> ;
                       rdfs:comment "RainIntensityBoundary - Limit of a State of the RainIntensity Property. The value of the limit is a quantitative data. To store the value, the boundaryValue data property is used."@en ;
                       rdfs:label "Rain Intensity Boundary"@en ;
                       <http://www.w3.org/2004/02/skos/core#example> "10 mm is the value of the Quantity_Low Boundary that distinguish the Light State from the Moderate State of the RainIntensity Property."@en .


###  http://www.w3id.org/def/irrig#RainIntensityDeduction
:RainIntensityDeduction rdf:type owl:Class ;
                        owl:equivalentClass [ owl:intersectionOf ( <http://www.w3id.org/def/caso#Deduction>
                                                                   [ rdf:type owl:Restriction ;
                                                                     owl:onProperty <http://www.w3.org/ns/sosa/hasFeatureOfInterest> ;
                                                                     owl:hasValue :FeatureOfInterest_Rain
                                                                   ]
                                                                   [ rdf:type owl:Restriction ;
                                                                     owl:onProperty <http://www.w3.org/ns/sosa/observedProperty> ;
                                                                     owl:hasValue :ObservableProperty_Rain_Intensity
                                                                   ]
                                                                 ) ;
                                              rdf:type owl:Class
                                            ] ;
                        rdfs:subClassOf <http://www.w3id.org/def/caso#Deduction> ;
                        rdfs:comment "RainIntensityDeduction - a defined class that represents act of carrying out an (Observation) Procedure to estimate a State of the irrig:Observable_Rain_Intensity of the irrig:FeatureOfInterest_Rain. The Observation may be performed by an inference engine. Links to a State to describe what the result is. Links to a TemporalEntity to indicate when the result is valid and for how many time."@en ;
                        rdfs:label "Rain Intensity Deduction"@en ;
                        <http://www.w3.org/2004/02/skos/core#example> "A RainIntensityDeduction that made by the Software_Agent Sensor based on observations made by a pluviometer sensor"@en .


###  http://www.w3id.org/def/irrig#RainIntensityState
:RainIntensityState rdf:type owl:Class ;
                    owl:equivalentClass [ owl:intersectionOf ( <http://www.w3id.org/def/caso#State>
                                                               [ rdf:type owl:Class ;
                                                                 owl:unionOf ( [ rdf:type owl:Restriction ;
                                                                                 owl:onProperty <http://www.w3id.org/def/caso#hasClosedLowerBoundary> ;
                                                                                 owl:someValuesFrom :RainIntensityBoundary
                                                                               ]
                                                                               [ rdf:type owl:Restriction ;
                                                                                 owl:onProperty <http://www.w3id.org/def/caso#hasOpenLowerBoundary> ;
                                                                                 owl:someValuesFrom :RainIntensityBoundary
                                                                               ]
                                                                             )
                                                               ]
                                                               [ rdf:type owl:Class ;
                                                                 owl:unionOf ( [ rdf:type owl:Restriction ;
                                                                                 owl:onProperty <http://www.w3id.org/def/caso#hasClosedUpperBoundary> ;
                                                                                 owl:someValuesFrom :RainIntensityBoundary
                                                                               ]
                                                                               [ rdf:type owl:Restriction ;
                                                                                 owl:onProperty <http://www.w3id.org/def/caso#hasOpenUpperBoundary> ;
                                                                                 owl:someValuesFrom :RainIntensityBoundary
                                                                               ]
                                                                             )
                                                               ]
                                                             ) ;
                                          rdf:type owl:Class
                                        ] ;
                    rdfs:subClassOf <http://www.w3id.org/def/caso#State> ;
                    rdfs:comment "RainIntensityState - A qualitative value of the RainIntensity Property which changes over time, summarizing a set of information about that Property. To evaluate the State of the Property, some Boundaries are defined."@en ;
                    rdfs:label "Rain Intensity State"@en ;
                    <http://www.w3.org/2004/02/skos/core#example> "Example: The State Light of the RainIntensity Property represents a fact that there is no enought rain to take any decision about irrigation."@en .


###  http://www.w3id.org/def/irrig#RainQuantityObservation
:RainQuantityObservation rdf:type owl:Class ;
                         owl:equivalentClass [ owl:intersectionOf ( <http://www.w3id.org/def/caso#Observation>
                                                                    [ rdf:type owl:Restriction ;
                                                                      owl:onProperty <http://www.w3.org/ns/sosa/hasFeatureOfInterest> ;
                                                                      owl:hasValue :FeatureOfInterest_Rain
                                                                    ]
                                                                    [ rdf:type owl:Restriction ;
                                                                      owl:onProperty <http://www.w3.org/ns/sosa/observedProperty> ;
                                                                      owl:hasValue :ObservableProperty_Rain_Quantity
                                                                    ]
                                                                  ) ;
                                               rdf:type owl:Class
                                             ] ;
                         rdfs:subClassOf <http://www.w3id.org/def/caso#Observation> ;
                         rdfs:comment "RainQuantityObservation - a defined class that represents act of carrying out an (Observation) Procedure to estimate or calculate a value of irrig:ObservableProperty_Rain_Quantity of the irrig:FeatureOfInterest_Rain. This act can be performed during the context acquisition or context processing phases of a context aware system. Links to a Sensor to describe what made the Observation and how; links to the irrig:ObservableProperty_Rain_Quantity to describe what the result is an estimate of, and to the irrig:FeatureOfInterest_Rain. When the act is made during the context processing phase the entry values of the procedure should be linked to the Observation by the wasDerivedFrom property."@en ;
                         rdfs:label "Rain Quantity Observation"@en ;
                         <http://www.w3.org/2004/02/skos/core#example> "A RainQuantityObservation that made by the Pluviometer Sensor."@en .


###  http://www.w3id.org/def/irrig#RainTotalQuantityPerDayObservation
:RainTotalQuantityPerDayObservation rdf:type owl:Class ;
                                    owl:equivalentClass [ owl:intersectionOf ( <http://www.w3id.org/def/caso#Observation>
                                                                               [ rdf:type owl:Restriction ;
                                                                                 owl:onProperty <http://www.w3.org/ns/sosa/hasFeatureOfInterest> ;
                                                                                 owl:hasValue :FeatureOfInterest_Rain
                                                                               ]
                                                                               [ rdf:type owl:Restriction ;
                                                                                 owl:onProperty <http://www.w3.org/ns/sosa/observedProperty> ;
                                                                                 owl:hasValue :ObservableProperty_Rain_TotalQuantityPerDay
                                                                               ]
                                                                             ) ;
                                                          rdf:type owl:Class
                                                        ] ;
                                    rdfs:subClassOf <http://www.w3id.org/def/caso#Observation> ;
                                    rdfs:comment "RainQuantityObservation - a defined that represents act of carrying out an (Observation) Procedure to estimate or calculate a value of the irrig:ObservableProperty_Rain_TotalQuantityPerDay of the irrig:FeatureOfInterest_Rain. This act can be performed during the context acquisition or context processing phases of a context aware system. Links to a Sensor to describe what made the Observation and how; links to the irrig:ObservableProperty_Rain_TotalQuantityPerDay to describe what the result is an estimate of, and to the irrig:FeatureOfInterest_Rain. When the act is made during the context processing phase the entry values of the procedure should be linked to the Observation by the wasDerivedFrom property."@en ;
                                    rdfs:label "Rain Total Quantity Per Day Observation"@en ;
                                    <http://www.w3.org/2004/02/skos/core#example> "A RainTotalQuantityPerDayObservation is computed by the SoftwareAgent Sensor based on observations made by a pluviometer."@en .


###  http://www.w3id.org/def/irrig#RootZoneAverageMoisturePerDayObservation
:RootZoneAverageMoisturePerDayObservation rdf:type owl:Class ;
                                          owl:equivalentClass [ owl:intersectionOf ( <http://www.w3id.org/def/caso#Observation>
                                                                                     [ rdf:type owl:Restriction ;
                                                                                       owl:onProperty <http://www.w3.org/ns/sosa/hasFeatureOfInterest> ;
                                                                                       owl:hasValue :FeatureOfInterest_RootZone
                                                                                     ]
                                                                                     [ rdf:type owl:Restriction ;
                                                                                       owl:onProperty <http://www.w3.org/ns/sosa/observedProperty> ;
                                                                                       owl:hasValue :ObservableProperty_RootZone_AverageMoisturePerDay
                                                                                     ]
                                                                                   ) ;
                                                                rdf:type owl:Class
                                                              ] ;
                                          rdfs:subClassOf <http://www.w3id.org/def/caso#Observation> ;
                                          rdfs:comment "RootZoneAverageMoisturePerDayObservation - a defined class that represents act of carrying out an (Observation) Procedure to estimate or calculate a value of the irrig:ObservableProperty_RootZone_AverageMoisturePerDay  of the  irrig:FeatureOfInterest_RootZone. This act can be performed during the context acquisition or context processing phases of a context aware system. Links to a Sensor to describe what made the Observation and how; links to the irrig:ObservableProperty_RootZone_AverageMoisturePerDay to describe what the result is an estimate of, and to the irrig:FeatureOfInterest_RootZone. When the act is made during the context processing phase the entry values of the procedure should be linked to the Observation by the wasDerivedFrom property."@en ;
                                          rdfs:label "Root Zone Moisture Per Day Observation"@en ;
                                          <http://www.w3.org/2004/02/skos/core#example> "A RootZoneAverageMoisturePerDayObservation is computed by the SoftwareAgent Sensor."@en .


###  http://www.w3id.org/def/irrig#RootZoneMoistureLevelBoundary
:RootZoneMoistureLevelBoundary rdf:type owl:Class ;
                               rdfs:subClassOf <http://www.w3id.org/def/caso#Boundary> ;
                               rdfs:comment "RootZoneMoistureLevelBoundary - Limit of a State of the RootZoneMoistureLevel Property. The value of the limit is a quantitative data. To store the value, the boundaryValue data property is used."@en ;
                               rdfs:label "Root Zone Moisture Level Boundary"@en ;
                               <http://www.w3.org/2004/02/skos/core#example> "10 cbar is the value of the Tension_VeryHigh Boundary that distinguish the Saturated State from the VeryHigh State of the RootZoneMoistureLevel Property."@en .


###  http://www.w3id.org/def/irrig#RootZoneMoistureLevelDeduction
:RootZoneMoistureLevelDeduction rdf:type owl:Class ;
                                owl:equivalentClass [ owl:intersectionOf ( <http://www.w3id.org/def/caso#Deduction>
                                                                           [ rdf:type owl:Restriction ;
                                                                             owl:onProperty <http://www.w3.org/ns/sosa/hasFeatureOfInterest> ;
                                                                             owl:hasValue :FeatureOfInterest_RootZone
                                                                           ]
                                                                           [ rdf:type owl:Restriction ;
                                                                             owl:onProperty <http://www.w3.org/ns/sosa/observedProperty> ;
                                                                             owl:hasValue :ObservableProperty_RootZone_MoistureLevel
                                                                           ]
                                                                         ) ;
                                                      rdf:type owl:Class
                                                    ] ;
                                rdfs:subClassOf <http://www.w3id.org/def/caso#Deduction> ;
                                rdfs:comment "RootZoneMoistureLevelDeduction - a defined class represents act of carrying out an (Observation) Procedure to estimate a State of the irrig:ObservableProperty_RootZone_MoistureLevel of the irrig:FeatureOfInterest_RootZone. The Observation may be performed by an inference engine. Links to a State to describe what the result is. Links to a TemporalEntity to indicate when the result is valid and for how many time."@en ;
                                rdfs:label "Root Zone Moisture Level Deduction"@en ;
                                <http://www.w3.org/2004/02/skos/core#example> "A RootZoneMoistureLevelDeduction that made by the Software_Agent."@en .


###  http://www.w3id.org/def/irrig#RootZoneMoistureLevelState
:RootZoneMoistureLevelState rdf:type owl:Class ;
                            owl:equivalentClass [ owl:intersectionOf ( <http://www.w3id.org/def/caso#State>
                                                                       [ rdf:type owl:Class ;
                                                                         owl:unionOf ( [ rdf:type owl:Restriction ;
                                                                                         owl:onProperty <http://www.w3id.org/def/caso#hasClosedLowerBoundary> ;
