#################################################################

###  http://www.w3.org/2004/02/skos/core#definition
<http://www.w3.org/2004/02/skos/core#definition> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#example
<http://www.w3.org/2004/02/skos/core#example> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#note
<http://www.w3.org/2004/02/skos/core#note> rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.ontology-of-units-of-measure.org/resource/om-2/hasNumericValue
<http://www.ontology-of-units-of-measure.org/resource/om-2/hasNumericValue> rdf:type owl:ObjectProperty ;
                                                                            rdfs:label "has numeric value"@en .


###  http://www.ontology-of-units-of-measure.org/resource/om-2/hasUnit
<http://www.ontology-of-units-of-measure.org/resource/om-2/hasUnit> rdf:type owl:ObjectProperty ;
                                                                    rdfs:subPropertyOf owl:topObjectProperty ;
                                                                    rdfs:range <http://www.ontology-of-units-of-measure.org/resource/om-2/Unit> ;
                                                                    rdfs:label "has unit"@en .


###  http://www.w3.org/ns/sosa/hosts
<http://www.w3.org/ns/sosa/hosts> rdf:type owl:ObjectProperty ;
                                  <http://schema.org/domainIncludes> <http://www.w3.org/ns/sosa/Platform> ;
                                  <http://schema.org/rangeIncludes> <http://www.w3.org/ns/sosa/Actuator> ,
                                                                    <http://www.w3.org/ns/sosa/Platform> ,
                                                                    <http://www.w3.org/ns/sosa/Sampler> ,
                                                                    <http://www.w3.org/ns/sosa/Sensor> ;
                                  rdfs:comment "Relation between a Platform and a Sensor, Actuator, Sampler, or Platform, hosted or mounted on it."@en ;
                                  rdfs:isDefinedBy <http://www.w3.org/ns/sosa/> ;
                                  rdfs:label "hosts"@en ;
                                  <http://www.w3.org/2004/02/skos/core#definition> "Relation between a Platform and a Sensor, Actuator, Sampler, or Platform, hosted or mounted on it."@en .


#################################################################
#    Data properties
#################################################################

###  http://www.ontology-of-units-of-measure.org/resource/om-2/hasNumericalValue
<http://www.ontology-of-units-of-measure.org/resource/om-2/hasNumericalValue> rdf:type owl:DatatypeProperty ;
                                                                              rdfs:label "has numerical value"@en .


#################################################################
#    Classes
#################################################################

###  http://www.ontology-of-units-of-measure.org/resource/om-2/Unit
<http://www.ontology-of-units-of-measure.org/resource/om-2/Unit> rdf:type owl:Class ;
                                                                 rdfs:comment "A unit of measure is a definite magnitude of a quantity, defined and adopted by convention or by law. It is used as a standard for measurement of the same quantity, where any other value of the quantity can be expressed as a simple multiple of the unit. For example, length is a quantity; the metre is a unit of length that represents a definite predetermined length. When we say 10 metre (or 10 m), we actually mean 10 times the definite predetermined length called \"metre\"."@en ;
                                                                 rdfs:label "Unit"@en .


###  http://www.w3.org/ns/sosa/Platform
<http://www.w3.org/ns/sosa/Platform> rdf:type owl:Class ;
                                     rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                       owl:onProperty <http://www.w3.org/ns/sosa/hosts> ;
                                                       owl:allValuesFrom <http://www.w3.org/ns/ssn/System>
                                                     ] ;
                                     rdfs:comment "A Platform is an entity that hosts other entities, particularly Sensors, Actuators, Samplers, and other Platforms."@en ;
                                     rdfs:isDefinedBy <http://www.w3.org/ns/sosa/> ;
                                     rdfs:label "Platform"@en ;
                                     <http://www.w3.org/2004/02/skos/core#definition> "A Platform is an entity that hosts other entities, particularly Sensors, Actuators, Samplers, and other Platforms."@en ;
                                     <http://www.w3.org/2004/02/skos/core#example> "A post, buoy, vehicle, ship, aircraft, satellite, cell-phone, human or animal may act as platforms for (technical or biological) sensors or actuators."@en .


###  http://www.w3id.org/def/irrig#CropGrowthDeduction
:CropGrowthDeduction rdf:type owl:Class ;
                     owl:equivalentClass [ owl:intersectionOf ( <http://www.w3id.org/def/caso#Deduction>
                                                                [ rdf:type owl:Restriction ;
                                                                  owl:onProperty <http://www.w3.org/ns/sosa/hasFeatureOfInterest> ;
                                                                  owl:hasValue :FeatureOfInterest_Crop
                                                                ]
                                                                [ rdf:type owl:Restriction ;
                                                                  owl:onProperty <http://www.w3.org/ns/sosa/observedProperty> ;
                                                                  owl:hasValue :ObservableProperty_Crop_Growth
                                                                ]
                                                              ) ;
                                           rdf:type owl:Class
                                         ] ;
                     rdfs:subClassOf <http://www.w3id.org/def/caso#Deduction> ;
                     rdfs:comment "The inference result that determine the crop growth state at a specific time."@en ;
                     rdfs:label "Crop Growth Deduction"@en .


###  http://www.w3id.org/def/irrig#CropGrowthObservation
:CropGrowthObservation rdf:type owl:Class ;
                       owl:equivalentClass [ owl:intersectionOf ( <http://www.w3id.org/def/caso#Observation>
                                                                  [ rdf:type owl:Restriction ;
                                                                    owl:onProperty <http://www.w3.org/ns/sosa/hasFeatureOfInterest> ;
                                                                    owl:hasValue :FeatureOfInterest_Crop
                                                                  ]
                                                                  [ rdf:type owl:Restriction ;
                                                                    owl:onProperty <http://www.w3.org/ns/sosa/observedProperty> ;
                                                                    owl:hasValue :ObservableProperty_Crop_Stage
                                                                  ]
                                                                ) ;
                                             rdf:type owl:Class
                                           ] ;
                       rdfs:subClassOf <http://www.w3id.org/def/caso#Observation> ;
                       rdfs:comment "The act that a sensor carries out an crop growth observation."@en ;
                       rdfs:label "Crop growth observation"@en .


###  http://www.w3id.org/def/irrig#CropGrowthState
:CropGrowthState rdf:type owl:Class ;
                 rdfs:subClassOf <http://www.w3id.org/def/caso#State> ;
                 rdfs:comment "Situation that a crop growth property has qualitative data that distinguish with other situations. This is a definitive class including the following states: init, v2, v7, v7d25, r1, r1d15, r5, r5hg45."@en ;
                 rdfs:label "Crop Growth State"@en .


###  http://www.w3id.org/def/irrig#CropWaterNeedDeduction
:CropWaterNeedDeduction rdf:type owl:Class ;
                        owl:equivalentClass [ owl:intersectionOf ( <http://www.w3id.org/def/caso#Deduction>
                                                                   [ rdf:type owl:Restriction ;
                                                                     owl:onProperty <http://www.w3.org/ns/sosa/hasFeatureOfInterest> ;
                                                                     owl:hasValue :FeatureOfInterest_Crop
                                                                   ]
                                                                   [ rdf:type owl:Restriction ;
                                                                     owl:onProperty <http://www.w3.org/ns/sosa/observedProperty> ;
                                                                     owl:hasValue :ObservableProperty_Crop_WaterNeed
                                                                   ]
                                                                 ) ;
                                              rdf:type owl:Class
                                            ] ;
                        rdfs:subClassOf <http://www.w3id.org/def/caso#Deduction> ;
                        rdfs:comment "The inference result that determine the crop water need state at a specific time."@en ;
                        rdfs:label "Crop Water Need Deduction"@en .


###  http://www.w3id.org/def/irrig#CropWaterNeedState
:CropWaterNeedState rdf:type owl:Class ;
                    rdfs:subClassOf <http://www.w3id.org/def/caso#State> ;
                    rdfs:comment "Situation that a crop water need property has qualitative data that distinguish with other situations. This is a definitive class including the following states: init, no, yes, wait."@en ;
                    rdfs:label "Crop Water Need State"@en .


###  http://www.w3id.org/def/irrig#GrowthProperty
:GrowthProperty rdf:type owl:Class ;
                rdfs:subClassOf <http://www.w3.org/ns/ssn/Property> ;
                rdfs:comment "Property dedicated to the growth of plants."@en ;
                rdfs:label "Growth property"@en .


###  http://www.w3id.org/def/irrig#IntensityProperty
:IntensityProperty rdf:type owl:Class ;
                   rdfs:subClassOf <http://www.w3.org/ns/ssn/Property> ;
                   rdfs:comment "Property dedicated to the intensity of rainfall."@en ;
                   rdfs:label "Intensity property"@en .


###  http://www.w3id.org/def/irrig#IrrigationActuation
:IrrigationActuation rdf:type owl:Class ;
                     owl:equivalentClass [ rdf:type owl:Class ;
                                           owl:unionOf ( [ owl:intersectionOf ( <http://www.w3id.org/def/caso#Actuation>
                                                                                [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                                                         owl:onProperty <http://www.w3.org/ns/sosa/hasFeatureOfInterest> ;
                                                                                                         owl:hasValue :FeatureOfInterest_Soil60cmDepth
                                                                                                       ]
                                                                                                       [ rdf:type owl:Restriction ;
                                                                                                         owl:onProperty <http://www.w3.org/ns/sosa/observedProperty> ;
                                                                                                         owl:hasValue :ObservableProperty_Soil60cmDepth_Moisture
                                                                                                       ]
                                                                                                     ) ;
                                                                                  rdf:type owl:Class
                                                                                ]
                                                                              ) ;
                                                           rdf:type owl:Class
                                                         ]
                                                         [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                                  owl:onProperty <http://www.w3.org/ns/sosa/hasFeatureOfInterest> ;
                                                                                  owl:hasValue :FeatureOfInterest_Soil30cmDepth
                                                                                ]
                                                                                [ rdf:type owl:Restriction ;
                                                                                  owl:onProperty <http://www.w3.org/ns/sosa/observedProperty> ;
                                                                                  owl:hasValue :ObservableProperty_Soil30cmDepth_Moisture
                                                                                ]
                                                                              ) ;
                                                           rdf:type owl:Class
                                                         ]
                                                       )
                                         ] ;
                     rdfs:subClassOf <http://www.w3id.org/def/caso#Actuation> ;
                     rdfs:comment "The act that an actuator carries out an irrigation action."@en ;
                     rdfs:label "Irrigation actuation"@en .


###  http://www.w3id.org/def/irrig#MoistureProperty
:MoistureProperty rdf:type owl:Class ;
                  rdfs:subClassOf <http://www.w3.org/ns/ssn/Property> ;
                  rdfs:comment "Property dedicated to the moisture in the soil."@en ;
                  rdfs:label "Moisture property"@en .


###  http://www.w3id.org/def/irrig#RainIntensittyState
:RainIntensittyState rdf:type owl:Class ;
                     owl:equivalentClass [ owl:intersectionOf ( <http://www.w3id.org/def/caso#State>
                                                                [ rdf:type owl:Class ;
                                                                  owl:unionOf ( [ rdf:type owl:Restriction ;
                                                                                  owl:onProperty <http://www.w3id.org/def/caso#hasClosedLowerBoundary> ;
                                                                                  owl:someValuesFrom :RainIntensityBoundary
                                                                                ]
                                                                                [ rdf:type owl:Restriction ;
                                                                                  owl:onProperty <http://www.w3id.org/def/caso#hasOpenLowerBoundary> ;
                                                                                  owl:someValuesFrom :RainIntensityBoundary
                                                                                ]
                                                                              )
                                                                ]
                                                                [ rdf:type owl:Class ;
                                                                  owl:unionOf ( [ rdf:type owl:Restriction ;
                                                                                  owl:onProperty <http://www.w3id.org/def/caso#hasClosedUpperBoundary> ;
                                                                                  owl:someValuesFrom :RainIntensityBoundary
                                                                                ]
                                                                                [ rdf:type owl:Restriction ;
                                                                                  owl:onProperty <http://www.w3id.org/def/caso#hasOpenUpperBoundary> ;
                                                                                  owl:someValuesFrom :RainIntensityBoundary
                                                                                ]
                                                                              )
                                                                ]
                                                              ) ;
                                           rdf:type owl:Class
                                         ] ;
                     rdfs:subClassOf <http://www.w3id.org/def/caso#State> ;
                     rdfs:comment "Situation that a rain intensity level has qualitative data that distinguish with other situation. This is a definitive class including the following states: init, light, moderate, heavy."@en ;
                     rdfs:label "Rain Intensity State"@en .


###  http://www.w3id.org/def/irrig#RainIntensityBoundary
:RainIntensityBoundary rdf:type owl:Class ;
                       rdfs:subClassOf <http://www.w3id.org/def/caso#Boundary> ;
                       rdfs:comment "Limits to define a rain intensity state and distinguish this state to other states."@en ;
                       rdfs:label "Rain Intensity Boundary"@en .


###  http://www.w3id.org/def/irrig#RainIntensityDeduction
:RainIntensityDeduction rdf:type owl:Class ;
