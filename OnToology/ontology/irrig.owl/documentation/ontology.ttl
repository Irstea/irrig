                                                                                         owl:onProperty <http://www.w3id.org/def/caso#hasOpenLowerBoundary> ;
                                                                                         owl:someValuesFrom :RootZoneMoistureLevelBoundary
                                                                                       ]
                                                                                     )
                                                                       ]
                                                                       [ rdf:type owl:Class ;
                                                                         owl:unionOf ( [ rdf:type owl:Restriction ;
                                                                                         owl:onProperty <http://www.w3id.org/def/caso#hasClosedUpperBoundary> ;
                                                                                         owl:someValuesFrom :RootZoneMoistureLevelBoundary
                                                                                       ]
                                                                                       [ rdf:type owl:Restriction ;
                                                                                         owl:onProperty <http://www.w3id.org/def/caso#hasOpenUpperBoundary> ;
                                                                                         owl:someValuesFrom :RootZoneMoistureLevelBoundary
                                                                                       ]
                                                                                     )
                                                                       ]
                                                                     ) ;
                                                  rdf:type owl:Class
                                                ] ;
                            rdfs:subClassOf <http://www.w3id.org/def/caso#State> ;
                            rdfs:comment "Situation that a root zone moisture level has qualitative data that distinguish with other situation. This is a definitive class including the following states: init, saturated, very high, high, average, low, very low, dry."@en ;
                            rdfs:label "Root Zone Moisture Level State"@en .


